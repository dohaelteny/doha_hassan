<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="help_desk_users_inherit_module_name" model="ir.ui.view">
        <field name="name">res.usrs.view.form.inherit</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">
            <div name="button_box" position="inside">
                <button class="oe_stat_button" type="object" name="get_tickets">
                    <field string="Tickets" name="count_ticket" widget="statinfo"/>
                </button>
            </div>
        </field>
    </record>

</odoo>
